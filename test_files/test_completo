var c := 1;

fun somma(integer a, integer b): integer
	integer result;
	result  :=  a + b + c;
	if result > 100 then
		?. "Valore grande";
	else
		?. "Valore piccolo";
	end if;
	return result;
end fun;


main
	integer a := 10, b := 22;
	integer risultato;
	risultato := somma(a, b);
	?. risultato;
end main;