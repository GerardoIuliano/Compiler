
fun ritornoReale(real r) : real
    return r * 2;
end fun;

fun ritornoBool(bool b) : bool
    return not b;
end fun;

fun ritornoStringa(string s) : string
    return s & "Aggiunta";
end fun;

fun ritornoVoid()
end fun;

main
    bool boolean := ritornoBool(true);
    string stringa := ritornoStringa("Stringa");
    ? ritornoVoid();
end main;